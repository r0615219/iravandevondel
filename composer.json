{
  "require": {
    "craftcms/cms": "^3.0.0",
    "vlucas/phpdotenv": "^2.4.0",
    "studioespresso/craft-dumper": "2.0.0",
    "rias/craft-position-fieldtype": "1.0.14",
    "craftcms/redactor": "2.3.3.2",
    "ether/seo": "3.5.4",
    "verbb/super-table": "2.1.20",
    "sebastianlenz/linkfield": "1.0.15",
    "verbb/expanded-singles": "^1.0",
    "mikestecker/craft-videoembedder": "^1.1",
    "superbig/craft3-http2serverpush": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
